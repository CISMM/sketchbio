<?xml version="1.0" encoding="UTF-8"?>
<SketchBioDeviceConfig fileVersion="0.0">
  <vrpnFile>/playpen/sketchbio/vrpn-install/bin/vrpn.cfg</vrpnFile>
	<device name="razer">
		<buttons num="16"/>
		<analogs num="6"/>
	</device>
	<device name="filteredRazer">
		<tracker channel="0" hand="left"/>
		<tracker channel="1" hand="right"/>
	</device>
	<trackerInputTransform matrix="0.0625 0 0 0 0 0.0625 0 0 0 0 0.0625 0 0 0 0 1" />
	<modeSwitchButton deviceName="razer" buttonNumber="1" />
	<mode name="Edit Objects" defaultSelection="Objects">
		<button deviceName="razer" buttonNumber="0" hand="left" function="redo" />
		<button deviceName="razer" buttonNumber="2" hand="left" function="toggleCollisionChecks" />
		<button deviceName="razer" buttonNumber="3" hand="left" function="toggleSpringsEnabled" />
		<button deviceName="razer" buttonNumber="4" hand="left" function="zoom" />
		<button deviceName="razer" buttonNumber="5" hand="left" function="resetCameraAngle" />
		<button deviceName="razer" buttonNumber="6" hand="left" function="grabObjectOrWorld" />
		<button deviceName="razer" buttonNumber="8" hand="right" function="undo" />
		<button deviceName="razer" buttonNumber="9" hand="right" function="createCrystalByExample" />
		<button deviceName="razer" buttonNumber="10" hand="right" function="lockTransforms" />
		<button deviceName="razer" buttonNumber="11" hand="right" function="setTransform" />
		<button deviceName="razer" buttonNumber="12" hand="right" function="deleteObject" />
		<button deviceName="razer" buttonNumber="14" hand="right" function="grabObjectOrWorld" />
		<analog deviceName="razer" analogNumber="0" type="normalize">
			<interval min="-1" max="1" hand="left" function="rotateCameraYaw" />
		</analog>
		<analog deviceName="razer" analogNumber="1" type="normalize">
			<interval min="-1" max="1" hand="left" function="rotateCameraPitch" />
		</analog>
		<analog deviceName="razer" analogNumber="2" type="normalize">
			<interval min="0" max="1" hand="left" function="setCrystalByExampleCopies" />
		</analog>
		<analog deviceName="razer" analogNumber="3" type="normalize">
			<interval min="-1" max="1" hand="right" function="moveAlongTimeline" />
		</analog>
		<analog deviceName="razer" analogNumber="4" type="threshold">
			<threshold pressedWhen="greater than" value="0.25" hand="right" function="selectChildOfCurrent" />
			<threshold pressedWhen="less than" value="-0.25" hand="right" function="selectParentOfCurrent" />
		</analog>
	</mode>
	<mode name="Edit Springs" defaultSelection="Connectors">
		<button deviceName="razer" buttonNumber="0" hand="left" function="redo" />
		<button deviceName="razer" buttonNumber="2" hand="left" function="toggleCollisionChecks" />
		<button deviceName="razer" buttonNumber="3" hand="left" function="toggleSpringsEnabled" />
		<button deviceName="razer" buttonNumber="4" hand="left" function="zoom" />
		<button deviceName="razer" buttonNumber="5" hand="left" function="resetCameraAngle" />
		<button deviceName="razer" buttonNumber="6" hand="left" function="grabConnectorOrWorld" />
		<button deviceName="razer" buttonNumber="8" hand="right" function="undo" />
		<button deviceName="razer" buttonNumber="9" hand="right" function="deleteConnector" />
		<button deviceName="razer" buttonNumber="10" hand="right" function="snapConnectorToTerminus" />
		<button deviceName="razer" buttonNumber="11" hand="right" function="createSpring" />
		<button deviceName="razer" buttonNumber="12" hand="right" function="createTransparentConnector" />
		<button deviceName="razer" buttonNumber="14" hand="right" function="grabConnectorOrWorld" />
		<analog deviceName="razer" analogNumber="0" type="normalize">
			<interval min="-1" max="1" hand="left" function="rotateCameraYaw" />
		</analog>
		<analog deviceName="razer" analogNumber="1" type="normalize">
			<interval min="-1" max="1" hand="left" function="rotateCameraPitch" />
		</analog>
		<analog deviceName="razer" analogNumber="3" type="normalize">
			<interval min="-1" max="1" hand="right" function="moveAlongTimeline" />
		</analog>
		<analog deviceName="razer" analogNumber="4" type="threshold">
			<threshold pressedWhen="greater than" value="0.25" hand="right" function="selectChildOfCurrent" />
			<threshold pressedWhen="less than" value="-0.25" hand="right" function="selectParentOfCurrent" />
		</analog>
	</mode>
</SketchBioDeviceConfig>

